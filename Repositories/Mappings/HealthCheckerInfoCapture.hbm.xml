<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Domain.Core"
                   namespace="Domain.Core">

  <class name="HealthCheckerInfoCapture">
    <id name="Id">
      <generator class="guid" />
    </id>
    <property name="CPUUsage" />
    <property name="MemoryUsage" />
    <property name="Time" />
    <property name="AverageTime" />
    <property name="Type" />
  </class>
  

</hibernate-mapping>